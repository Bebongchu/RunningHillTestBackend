    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>7aecb26e-d521-418f-a18c-68fd84ee260b</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.SqlClient" Version="3.0.1" />
